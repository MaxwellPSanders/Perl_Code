#this program has the simple job of finding dot

#open the test file, grab everything and close it
open TEST, "regtest.txt";
chomp(@lines = <TEST>);
close TEST;

#go through each line and test
foreach (@lines){
    #check to find the dot
    if(/\./){
        print $_, "\n";
    }
}
